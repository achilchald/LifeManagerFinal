<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="AddProjectAnchor" prefHeight="528.0" prefWidth="361.0" style="-fx-background-color: #fcfcfc; -fx-background-radius: 10px 10px 10px 10px;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.proj_add_Controller">
   <children>
      <Label layoutX="127.0" layoutY="4.0" text="New Project">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TabPane fx:id="TabProject" focusTraversable="false" layoutX="1.0" layoutY="34.0" prefHeight="487.0" prefWidth="360.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab closable="false" text="Project Details">
               <content>
                  <AnchorPane prefHeight="448.0" prefWidth="282.0">
                     <children>
                        <TextField fx:id="NameF" layoutX="104.0" layoutY="49.0" />
                        <TextField fx:id="PriceF" layoutX="104.0" layoutY="123.0" />
                        <Label layoutY="53.0" text="Name" />
                        <Label layoutX="3.0" layoutY="86.0" text="Date" />
                        <Label layoutX="3.0" layoutY="127.0" text="Price" />
                        <Label layoutX="4.0" layoutY="14.0" text="Project Details">
                           <font>
                              <Font name="System Bold Italic" size="12.0" />
                           </font>
                        </Label>
                        <Line endX="174.0" layoutX="107.0" layoutY="41.0" startX="-106.0" />
                        <DatePicker fx:id="date" layoutX="104.0" layoutY="82.0" prefHeight="25.0" prefWidth="149.0" />
                        <Button fx:id="AddB" layoutX="212.0" layoutY="187.0" mnemonicParsing="false" onAction="#pressed" text="Next" />
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
          <Tab closable="false" text="Workers">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="361.0" prefWidth="422.0">
                     <children>
                        <Line endX="264.0" layoutX="24.0" layoutY="27.0" startX="-20.0" />
                        <Label layoutX="8.0" text="Workers">
                           <font>
                              <Font name="System Bold Italic" size="12.0" />
                           </font>
                        </Label>
                        <Button fx:id="addWorkerBtn" layoutX="8.0" layoutY="272.0" mnemonicParsing="false" onAction="#addWorkerToProject" text="Add Worker" />
                        <ComboBox fx:id="workerscombo" layoutX="4.0" layoutY="39.0" prefWidth="150.0" style="-fx-background-color: #FFFFFF;" />
                        <VBox fx:id="workersbox" layoutX="12.0" layoutY="83.0" prefHeight="186.0" prefWidth="285.0" />
                        <Button fx:id="AddW" layoutX="146.0" layoutY="272.0" mnemonicParsing="false" onAction="#AddedWorker" text="Next" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab text="Tasks">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="464.0" prefWidth="360.0">
                     <children>
                        <Button fx:id="AddToDoBtn" layoutX="257.0" layoutY="243.0" mnemonicParsing="false" onAction="#addTask" text="Add Task" />
                        <VBox fx:id="todobox" layoutX="15.0" layoutY="297.0" prefHeight="121.0" prefWidth="331.0" />
                        <TextField fx:id="nametask" layoutX="84.0" layoutY="27.0" prefHeight="25.0" prefWidth="153.0" />
                        <Label layoutX="9.0" layoutY="31.0" text="Task name" />
                        <Label layoutX="19.0" layoutY="224.0" text="Desc" />
                        <TextField fx:id="desctask" layoutX="86.0" layoutY="224.0" prefHeight="62.0" prefWidth="149.0" />
                        <ComboBox fx:id="taskCombo" layoutX="85.0" layoutY="72.0" prefHeight="25.0" prefWidth="152.0" />
                        <Label layoutX="12.0" layoutY="76.0" text="Worker" />
                        <Button fx:id="AddT" layoutX="157.0" layoutY="425.0" mnemonicParsing="false" onAction="#finish" text="Finish" />
                        <Label layoutX="14.0" layoutY="178.0" text="Category" />
                        <ComboBox fx:id="taskCategory" layoutX="85.0" layoutY="174.0" prefHeight="25.0" prefWidth="150.0" />
                        <Label layoutX="13.0" layoutY="127.0" text="Due Date" />
                        <DatePicker fx:id="datePicker" layoutX="86.0" layoutY="123.0" prefHeight="25.0" prefWidth="150.0" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
